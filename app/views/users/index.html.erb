<div class="container">

<table>
  <thead>
    <tr>
      <th>Admin</th>
      <th>Name</th>
      <th>Position</th>
      <th>Department</th>
      <th>Workload</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @users.each do |user| %>
      <tr>
        <td><%= user.admin %></td>
        <td><%= user.name %></td>
        <td><%= user.position %></td>
        <td><%= user.department %></td>
        <td><%= user.workload %></td>
        <td><%= link_to 'Show', user %></td>
        <td><%= link_to 'Edit', edit_user_path(user) %></td>
        <td><%= link_to 'Destroy', user, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New User', new_user_path %>


</div>